{
  "app": {
    "security": {
      "capabilities": [
        {
          "identifier": "default",
          "windows": ["main"],
          "permissions": ["core:event:allow-listen", "core:event:allow-unlisten"]
        }
      ],
      "csp": "default-src 'self'"
    }
  },
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "cd ../frontend && npm run dev",
    "devUrl": "http://localhost:3000",
    "frontendDist": "../frontend/build"
  },
  "bundle": {
    "active": true,
    "category": "Utility",
    "copyright": "",
    "externalBin": [],
    "icon": [],
    "linux": {
      "deb": {
        "depends": []
      }
    },
    "longDescription": "",
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    },
    "resources": [],
    "shortDescription": "The GameController for the RoboCup Standard Platform League",
    "targets": "all",
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    }
  },
  "identifier": "org.robocup.spl.game-controller",
  "mainBinaryName": "GameController",
  "productName": "GameController",
  "version": "4.0.0"
}
